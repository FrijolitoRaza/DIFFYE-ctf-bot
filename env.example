# DIFFYE-CTF Bot Configuration
# Copia este archivo como .env y configura tus valores

# Bot Token de Telegram
BOT_TOKEN=your_telegram_bot_token_here

# Base de datos PostgreSQL
DATABASE_URL=postgresql://username:password@localhost:5432/diffye_ctf

# Configuración del pool de conexiones
DB_MIN_CONNECTIONS=5
DB_MAX_CONNECTIONS=20

# IDs de administradores (separados por comas)
ADMIN_IDS=123456789,987654321

# Zona horaria
TIMEZONE=America/Argentina/Buenos_Aires

# Fechas del evento (formato: YYYY-MM-DD)
START_DATE=2024-09-15
END_DATE=2024-09-19

# Configuración de logging
LOG_LEVEL=INFO
LOG_FILE=logs/bot.log

# Configuración de rate limiting
RATE_LIMIT_MAX_CALLS=10
RATE_LIMIT_PERIOD=60


# Link de material para desafios
DESAFIO1= 

DESAFIO2=

DESAFIO3=

DESAFIO4=

DESAFIO5=

IMGFINAL=